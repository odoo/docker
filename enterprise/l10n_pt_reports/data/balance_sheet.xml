                                <field name="code">CP11</field>
                                <field name="action_id" eval="ref('action_account_financial_report_pt_pnl')"/>
                                <field name="sequence" eval="360"/>
                                <field name="account_codes_formula" eval="False"/>
                                <field name="expression_ids">
