                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">-3360</field>
                                    </record>
                                    <record id="l10n_kz_report_line_bs_current_liabilities_219" model="account.report.line">
