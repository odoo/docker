<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="l10_kr_bs" model="account.report">
        <field name="name">Balance Sheet</field>
        <field name="country_id" ref="base.kr"/>
        <field name="root_report_id" ref="account_reports.balance_sheet"/>
        <field name="filter_date_range" eval="False"/>
        <field name="filter_analytic_groupby" eval="True"/>
        <field name="filter_unfold_all" eval="True"/>
        <field name="filter_journals" eval="True"/>
        <field name="filter_multi_company">selector</field>
        <field name="default_opening_date_filter">today</field>
        <field name="column_ids">
            <record id="l10n_kr_bs_column" model="account.report.column">
                <field name="name">Balance</field>
                <field name="expression_label">balance</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="l10n_kr_bs_ta" model="account.report.line">
                <field name="name">Assets</field>
                <field name="code">KR_TA</field>
                <field name="hierarchy_level" eval="0"/>
                <field name="aggregation_formula">KR_CA.balance + KR_NCA.balance</field>
                <field name="children_ids">
                    <record id="l10n_kr_bs_ca" model="account.report.line">
                        <field name="name">Current Assets</field>
                        <field name="code">KR_CA</field>
                        <field name="aggregation_formula">KR_QA.balance + KR_IN.balance</field>
                        <field name="children_ids">
                            <record id="l10n_kr_bs_qa" model="account.report.line">
                                <field name="name">Quick Assets</field>
                                <field name="code">KR_QA</field>
                                <field name="aggregation_formula">KR_CCE.balance + KR_SI.balance + KR_TR.balance + KR_PE.balance + KR_DTACA.balance + KR_OCCE.balance</field>
                                <field name="children_ids">
                                    <record id="l10n_kr_bs_line_cce" model="account.report.line">
                                        <field name="name">Cash and Cash Equivalents</field>
                                        <field name="code">KR_CCE</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1111</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_si" model="account.report.line">
                                        <field name="name">Short-term Investments</field>
                                        <field name="code">KR_SI</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1112</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_tr" model="account.report.line">
                                        <field name="name">Trade Receivables</field>
                                        <field name="code">KR_TR</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1113</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_pe" model="account.report.line">
                                        <field name="name">Prepaid Expenses</field>
                                        <field name="code">KR_PE</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1114</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_dtaca" model="account.report.line">
                                        <field name="name">Deferred tax assets (Current Asset)</field>
                                        <field name="code">KR_DTACA</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1115</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_occe" model="account.report.line">
                                        <field name="name">Other Cash and Cash Equivalents</field>
                                        <field name="code">KR_OCCE</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1116</field>
                                    </record>
                                </field>
                            </record>
                            <record id="l10n_kr_bs_in" model="account.report.line">
                                <field name="name">Inventories</field>
                                <field name="code">KR_IN</field>
                                <field name="aggregation_formula">KR_M.balance + KR_FG.balance + KR_SFG.balance + KR_WIP.balance + KR_RM.balance + KR_S.balance + KR_OI.balance</field>
                                <field name="children_ids">
                                    <record id="l10n_kr_bs_line_m" model="account.report.line">
                                        <field name="name">Merchandise</field>
                                        <field name="code">KR_M</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1121</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_fg" model="account.report.line">
                                        <field name="name">Finished Goods</field>
                                        <field name="code">KR_FG</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1122</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_sfg" model="account.report.line">
                                        <field name="name">Semi-Finished Goods</field>
                                        <field name="code">KR_SFG</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1123</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_wip" model="account.report.line">
                                        <field name="name">Work In Progress</field>
                                        <field name="code">KR_WIP</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1124</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_rm" model="account.report.line">
                                        <field name="name">Raw Materials</field>
                                        <field name="code">KR_RM</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1125</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_s" model="account.report.line">
                                        <field name="name">Supplies</field>
                                        <field name="code">KR_S</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1126</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_oi" model="account.report.line">
                                        <field name="name">Other Inventories</field>
                                        <field name="code">KR_OI</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1127</field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                    <record id="l10n_kr_bs_nca" model="account.report.line">
                        <field name="name">Non-current assets</field>
                        <field name="code">KR_NCA</field>
                        <field name="aggregation_formula">KR_INV.balance + KR_PPE.balance + KR_IA.balance + KR_DTANCA.balance</field>
                        <field name="children_ids">
                            <record id="l10n_kr_bs_inv" model="account.report.line">
                                <field name="name">Investments</field>
                                <field name="code">KR_INV</field>
                                <field name="aggregation_formula">KR_INVP.balance + KR_LTS.balance + KR_INVEM.balance + KR_LTL.balance + KR_OINV.balance</field>
                                <field name="children_ids">
                                    <record id="l10n_kr_bs_line_invp" model="account.report.line">
                                        <field name="name">Investment property</field>
                                        <field name="code">KR_INVP</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1211</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_lts" model="account.report.line">
                                        <field name="name">Long-term securities</field>
                                        <field name="code">KR_LTS</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1212</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_invem" model="account.report.line">
                                        <field name="name">Investments accounted for using equity method</field>
                                        <field name="code">KR_INVEM</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1213</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_ltl" model="account.report.line">
                                        <field name="name">Long-term Loans</field>
                                        <field name="code">KR_LTL</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1214</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_oinv" model="account.report.line">
                                        <field name="name">Other Investments</field>
                                        <field name="code">KR_OINV</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1215</field>
                                    </record>
                                </field>
                            </record>
                            <record id="l10n_kr_bs_ppe" model="account.report.line">
                                <field name="name">Property, plant and equipment</field>
                                <field name="code">KR_PPE</field>
                                <field name="aggregation_formula">KR_LA.balance + KR_TNA.balance + KR_CIP.balance + KR_OTA.balance</field>
                                <field name="children_ids">
                                    <record id="l10n_kr_bs_line_la" model="account.report.line">
                                        <field name="name">Land</field>
                                        <field name="code">KR_LA</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1221</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_tna" model="account.report.line">
                                        <field name="name">Tangible Assets</field>
                                        <field name="code">KR_TNA</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1222</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_cip" model="account.report.line">
                                        <field name="name">Construction in progress</field>
                                        <field name="code">KR_CIP</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1223</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_ota" model="account.report.line">
                                        <field name="name">Other Tangible Assets</field>
                                        <field name="code">KR_OTA</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1224</field>
                                    </record>
                                </field>
                            </record>
                            <record id="l10n_kr_bs_ia" model="account.report.line">
                                <field name="name">Intangible Assets</field>
                                <field name="code">KR_IA</field>
                                <field name="aggregation_formula">KR_G.balance + KR_IPR.balance + KR_DC.balance + KR_OIA.balance</field>
                                <field name="children_ids">
                                    <record id="l10n_kr_bs_line_g" model="account.report.line">
                                        <field name="name">Goodwill</field>
                                        <field name="code">KR_G</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1231</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_ipr" model="account.report.line">
                                        <field name="name">Industrial property rights</field>
                                        <field name="code">KR_IPR</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1232</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_dc" model="account.report.line">
                                        <field name="name">Development Cost</field>
                                        <field name="code">KR_DC</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1233</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_oia" model="account.report.line">
                                        <field name="name">Other Intangible Assets</field>
                                        <field name="code">KR_OIA</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1234</field>
                                    </record>
                                </field>
                            </record>
                            <record id="l10n_kr_bs_dtanca" model="account.report.line">
                                <field name="name">Deferred tax assets (Non Current)</field>
                                <field name="code">KR_DTANCA</field>
                                <field name="aggregation_formula">KR_DTANCAS.balance + KR_ONCA.balance</field>
                                <field name="children_ids">
                                    <record id="l10n_kr_bs_line_dtancas" model="account.report.line">
                                        <field name="name">Deferred tax assets (Non Current)</field>
                                        <field name="code">KR_DTANCAS</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1241</field>
                                    </record>
                                    <record id="l10n_kr_bs_line_onca" model="account.report.line">
                                        <field name="name">Other non-current Assets</field>
                                        <field name="code">KR_ONCA</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">1242</field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record id="l10n_kr_bs_l" model="account.report.line">
                <field name="name">Liabilities</field>
                <field name="code">KR_L</field>
                <field name="hierarchy_level" eval="0"/>
                <field name="aggregation_formula">KR_CL.balance + KR_NCL.balance</field>
                <field name="children_ids">
                    <record id="l10n_kr_bs_cl" model="account.report.line">
                        <field name="name">Current Liabilities</field>
                        <field name="code">KR_CL</field>
                        <field name="aggregation_formula">KR_STB.balance + KR_TPL.balance + KR_CTL.balance + KR_AC.balance + KR_DTLCL.balance + KR_OCL.balance</field>
                        <field name="children_ids">
                            <record id="l10n_kr_bs_line_stb" model="account.report.line">
                                <field name="name">Short-term borrowings</field>
                                <field name="code">KR_STB</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-211</field>
                            </record>
                            <record id="l10n_kr_bs_line_tp" model="account.report.line">
                                <field name="name">Trade Payables</field>
                                <field name="code">KR_TPL</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-212</field>
                            </record>
                            <record id="l10n_kr_bs_line_stl" model="account.report.line">
                                <field name="name">Current Tax Liabilities</field>
                                <field name="code">KR_CTL</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-213</field>
                            </record>
                            <record id="l10n_kr_bs_line_ac" model="account.report.line">
                                <field name="name">Accruals</field>
                                <field name="code">KR_AC</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-214</field>
                            </record>
                            <record id="l10n_kr_bs_line_dtlcl" model="account.report.line">
                                <field name="name">Deferred Tax Liabilities</field>
                                <field name="code">KR_DTLCL</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-215</field>
                            </record>
                            <record id="l10n_kr_bs_line_ocl" model="account.report.line">
                                <field name="name">Other Current Liabilities</field>
                                <field name="code">KR_OCL</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-216</field>
                            </record>
                        </field>
                    </record>
                    <record id="l10n_kr_bs_ncl" model="account.report.line">
                        <field name="name">Non-Current Liabilities</field>
                        <field name="code">KR_NCL</field>
                        <field name="aggregation_formula">KR_BW.balance + KR_BWW.balance + KR_CB.balance + KR_NCB.balance + KR_PEBO.balance + KR_LTWP.balance + KR_DTLNCL.balance + KR_ONCL.balance</field>
                        <field name="children_ids">
                            <record id="l10n_kr_bs_line_bw" model="account.report.line">
                                <field name="name">Bonds</field>
                                <field name="code">KR_BW</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-221</field>
                            </record>
                            <record id="l10n_kr_bs_line_bww" model="account.report.line">
                                <field name="name">Bonds With Warrants</field>
                                <field name="code">KR_BWW</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-222</field>
                            </record>
                            <record id="l10n_kr_bs_line_cb" model="account.report.line">
                                <field name="name">Convertible Bond</field>
                                <field name="code">KR_CB</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-223</field>
                            </record>
                            <record id="l10n_kr_bs_line_ncb" model="account.report.line">
                                <field name="name">Non-Current Borrowings</field>
                                <field name="code">KR_NCB</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-224</field>
                            </record>
                            <record id="l10n_kr_bs_line_pebo" model="account.report.line">
                                <field name="name">Post-Employment Benefit Obligations</field>
                                <field name="code">KR_PEBO</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-225</field>
                            </record>
                            <record id="l10n_kr_bs_line_ltwp" model="account.report.line">
                                <field name="name">Long-term Warranty Provision</field>
                                <field name="code">KR_LTWP</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-226</field>
                            </record>
                            <record id="l10n_kr_bs_line_dtlncl" model="account.report.line">
                                <field name="name">Deferred Tax Liabilities</field>
                                <field name="code">KR_DTLNCL</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-227</field>
                            </record>
                            <record id="l10n_kr_bs_line_oncl" model="account.report.line">
                                <field name="name">Other Non-Current Liabilities</field>
                                <field name="code">KR_ONCL</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="hierarchy_level" eval="7"/>
                                <field name="account_codes_formula">-228</field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record id="l10n_kr_bs_eq" model="account.report.line">
                <field name="name">Equity</field>
                <field name="code">KR_EQ</field>
                <field name="hierarchy_level" eval="0"/>
                <field name="aggregation_formula">KR_SC.balance + KR_CAPS.balance + KR_CAPA.balance + KR_AOCI.balance + KR_RE.balance</field>
                <field name="children_ids">
                    <record id="l10n_kr_bs_line_sc" model="account.report.line">
                        <field name="name">Share Capital</field>
                        <field name="code">KR_SC</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-31</field>
                    </record>
                    <record id="l10n_kr_bs_line_caps" model="account.report.line">
                        <field name="name">Capital Surplus</field>
                        <field name="code">KR_CAPS</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-32</field>
                    </record>
                    <record id="l10n_kr_bs_line_capa" model="account.report.line">
                        <field name="name">Capital Adjustments</field>
                        <field name="code">KR_CAPA</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-33</field>
                    </record>
                    <record id="l10n_kr_bs_line_aoci" model="account.report.line">
                        <field name="name">Accumulated Other Comprehensive Income</field>
                        <field name="code">KR_AOCI</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-34</field>
                    </record>
                    <record id="l10n_kr_bs_re" model="account.report.line">
                        <field name="name">Retained Earnings</field>
                        <field name="code">KR_RE</field>
                        <field name="aggregation_formula">KR_LR.balance + KR_VR.balance + KR_URE.balance</field>
                        <field name="children_ids">
                            <record id="l10n_kr_bs_line_lr" model="account.report.line">
                                <field name="name">Legal Reserve</field>
                                <field name="code">KR_LR</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">-351</field>
                            </record>
                            <record id="l10n_kr_bs_line_vr" model="account.report.line">
                                <field name="name">Voluntary Reserve</field>
                                <field name="code">KR_VR</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">-352</field>
                            </record>
                            <record id="l10n_kr_bs_ure" model="account.report.line">
                                <field name="name">Unappropriated Retained Earnings</field>
                                <field name="code">KR_URE</field>
                                <field name="aggregation_formula">KR_CURR_YEAR_EARNINGS.balance + KR_PREV_YEAR_EARNINGS.balance + KR_PREV_YEAR_URE.balance</field>
                                <field name="children_ids">
                                    <record id="l10n_kr_bs_line_curr_year_earnings" model="account.report.line">
                                        <field name="name">Current Year Unallocated Earnings</field>
                                        <field name="code">KR_CURR_YEAR_EARNINGS</field>
                                        <field name="expression_ids">
                                            <record id="l10n_kr_bs_line_curr_year_earnings_pnl" model="account.report.expression">
                                                <field name="label">pnl</field>
                                                <field name="engine">aggregation</field>
                                                <field name="formula">KR_NEP.balance</field>
                                                <field name="date_scope">from_fiscalyear</field>
                                                <field name="subformula">cross_report</field>
                                            </record>
                                            <record id="l10n_kr_bs_line_curr_year_earnings_alloc" model="account.report.expression">
                                                <field name="label">alloc</field>
                                                <field name="engine">domain</field>
                                                <field name="formula" eval="[('account_id.account_type', '=', 'equity_unaffected')]"/>
                                                <field name="date_scope">from_fiscalyear</field>
                                                <field name="subformula">-sum</field>
                                            </record>
                                            <record id="l10n_kr_bs_line_curr_year_earnings_balance" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">aggregation</field>
                                                <field name="formula">KR_CURR_YEAR_EARNINGS.pnl + KR_CURR_YEAR_EARNINGS.alloc</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="l10n_kr_bs_line_prev_year_earnings" model="account.report.line">
                                        <field name="name">Previous Years Unallocated Earnings</field>
                                        <field name="code">KR_PREV_YEAR_EARNINGS</field>
                                        <field name="expression_ids">
                                            <record id="l10n_kr_bs_line_prev_year_earnings_allocated_earnings" model="account.report.expression">
                                                <field name="label">allocated_earnings</field>
                                                <field name="engine">domain</field>
                                                <field name="formula" eval="[('account_id.account_type', '=', 'equity_unaffected')]"/>
                                                <field name="subformula">-sum</field>
                                                <field name="date_scope">from_beginning</field>
                                            </record>
                                            <record id="l10n_kr_bs_line_prev_year_earnings_balance_domain" model="account.report.expression">
                                                <field name="label">balance_domain</field>
                                                <field name="engine">domain</field>
                                                <field name="formula" eval="[('account_id.account_type', 'in', ('income', 'income_other', 'expense_direct_cost', 'expense', 'expense_depreciation'))]"/>
                                                <field name="subformula">-sum</field>
                                                <field name="date_scope">from_beginning</field>
                                            </record>
                                            <record id="l10n_kr_bs_line_prev_year_earnings_balance" model="account.report.expression">
                                                <field name="label">balance</field>
                                                <field name="engine">aggregation</field>
                                                <field name="formula">KR_PREV_YEAR_EARNINGS.balance_domain + KR_PREV_YEAR_EARNINGS.allocated_earnings - KR_CURR_YEAR_EARNINGS.balance</field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="l10n_kr_bs_line_prev_year_ure" model="account.report.line">
                                        <field name="name">Previous Years Unappropriated Retained Earnings</field>
                                        <field name="code">KR_PREV_YEAR_URE</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">-353</field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record id="l10n_kr_bs_le" model="account.report.line">
                <field name="name">Liabilities + Equity</field>
                <field name="code">KR_LE</field>
                <field name="hierarchy_level" eval="0"/>
                <field name="aggregation_formula">KR_L.balance + KR_EQ.balance</field>
            </record>
        </field>
    </record>
</odoo>
