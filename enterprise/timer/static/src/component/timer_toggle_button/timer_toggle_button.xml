<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="timer.ToggleButton">
        <button type="button"
                t-on-click.stop.prevent="onClick"
                t-attf-class="px-3 py-1 border border-0 rounded-1 text-center {{buttonClass}}"
                t-att-title="title" t-att-aria-label="title">
            <i t-att-class="iconClass" t-att-title="title"/>
        </button>
    </t>

</templates>
