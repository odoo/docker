				<xsd:simpleType>
					<xsd:annotation>
						<xsd:documentation>
							Booking Time
						</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:duration" />
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CurrencyCode" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="3" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ExchangeRate" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
