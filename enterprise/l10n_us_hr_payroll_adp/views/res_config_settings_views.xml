<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.l10n_us_hr_payroll_adp</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="45"/>
        <field name="inherit_id" ref="l10n_us_hr_payroll.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='l10n_us_suta_text']" position="after">
                <group class="mt16">
                    <field name="l10n_us_adp_code"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
