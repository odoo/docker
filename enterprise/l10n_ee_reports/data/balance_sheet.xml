                                    <record id="account_financial_report_bs_lt_tax_prov" model="account.report.line">
                                        <field name="name">Tax Provisions</field>
                                        <field name="code">EE_BS_2121</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">-2121</field>
                                    </record>
                                    <record id="account_financial_report_bs_lt_other_prov" model="account.report.line">
                                        <field name="name">Other Provisions</field>
                                        <field name="code">EE_BS_2122</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">-2122</field>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_report_bs_lt_gov_grants" model="account.report.line">
                                <field name="name">Government Grants</field>
                                <field name="code">EE_BS_213</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">-213</field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_bs_equity" model="account.report.line">
                <field name="name">EQUITY</field>
                <field name="code">EE_BS_3</field>
                <field name="aggregation_formula">EE_BS_300.balance + EE_BS_301.balance + EE_BS_302.balance + EE_BS_303.balance + EE_BS_304.balance + EE_BS_310.balance + EE_BS_311.balance + EE_BS_32.balance + EE_BS_330.balance + EE_BS_331.balance + EE_BS_UNALLOC_PL.balance</field>
                <field name="hierarchy_level">0</field>
                <field name="children_ids">
                    <record id="account_financial_report_bs_share_cap" model="account.report.line">
                        <field name="name">Share Capital (Nominal Value)</field>
                        <field name="code">EE_BS_300</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-300</field>
                    </record>
                    <record id="account_financial_report_bs_unreg_share_cap" model="account.report.line">
                        <field name="name">Unregistered Share Capital or Equity</field>
                        <field name="code">EE_BS_301</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-301</field>
                    </record>
                    <record id="account_financial_report_bs_unpaid_share_cap" model="account.report.line">
                        <field name="name">Unpaid Share Capital</field>
                        <field name="code">EE_BS_302</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-302</field>
                    </record>
                    <record id="account_financial_report_bs_share_prem" model="account.report.line">
                        <field name="name">Share Premium</field>
                        <field name="code">EE_BS_303</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-303</field>
                    </record>
                    <record id="account_financial_report_bs_own_shares" model="account.report.line">
                        <field name="name">Own Shares</field>
                        <field name="code">EE_BS_304</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-304</field>
                    </record>
                    <record id="account_financial_report_bs_stat_res_cap" model="account.report.line">
                        <field name="name">Statutory Reserve Capital</field>
                        <field name="code">EE_BS_310</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-310</field>
                    </record>
                    <record id="account_financial_report_bs_other_res" model="account.report.line">
                        <field name="name">Other Reserves</field>
                        <field name="code">EE_BS_311</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-311</field>
                    </record>
                    <record id="account_financial_report_bs_other_equity" model="account.report.line">
                        <field name="name">Other Equity</field>
                        <field name="code">EE_BS_32</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-32</field>
                    </record>
                    <record id="account_financial_report_bs_unalloc_pl" model="account.report.line">
                        <field name="name">Unallocated Profit/Loss</field>
                        <field name="code">EE_BS_UNALLOC_PL</field>
                        <field name="foldable" eval="True"/>
                        <field name="aggregation_formula">EE_BS_UNALLOC_PL_PREV.balance + EE_BS_UNALLOC_PL_CUR.balance</field>
                        <field name="children_ids">
                            <record id="account_financial_report_bs_unalloc_pl_prev" model="account.report.line">
                                <field name="name">Unallocated Profit/Loss from Previous Periods</field>
                                <field name="code">EE_BS_UNALLOC_PL_PREV</field>
                                <field name="expression_ids">
                                    <record id="account_financial_report_bs_unalloc_pl_prev_balance" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[
                                            '|',
                                            ('account_id.account_type', '=', 'equity_unaffected'),
                                            ('account_id.internal_group', 'in', ('income', 'expense')),
                                        ]"/>
                                        <field name="subformula">-sum</field>
                                        <field name="date_scope">to_beginning_of_fiscalyear</field>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_report_bs_unalloc_pl_cur" model="account.report.line">
                                <field name="name">Unallocated Profit/Loss for the Financial Year</field>
                                <field name="code">EE_BS_UNALLOC_PL_CUR</field>
                                <field name="action_id" ref="account_reports.action_account_report_pl"/>
                                <field name="expression_ids">
                                    <record id="account_financial_report_bs_unalloc_pl_cur_balance_unaff" model="account.report.expression">
                                        <field name="label">balance_unaff</field>
                                        <field name="engine">domain</field>
                                        <field name="formula" eval="[('account_id.account_type', '=', 'equity_unaffected')]"/>
                                        <field name="subformula">-sum</field>
                                        <field name="date_scope">from_fiscalyear</field>
                                    </record>
                                    <record id="account_financial_report_bs_unalloc_pl_cur_balance_pl" model="account.report.expression">
                                        <field name="label">balance_pl</field>
                                        <field name="engine">aggregation</field>
                                        <field name="formula">EE_PL_1_FINAL.balance</field>
                                        <field name="subformula">cross_report</field>
                                        <field name="date_scope">from_fiscalyear</field>
                                    </record>
                                    <record id="account_financial_report_bs_unalloc_pl_cur_balance" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">aggregation</field>
                                        <field name="formula">EE_BS_UNALLOC_PL_CUR.balance_unaff + EE_BS_UNALLOC_PL_CUR.balance_pl</field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_report_bs_pl_prev" model="account.report.line">
                        <field name="name">Retained Profit/Loss from Previous Periods</field>
                        <field name="code">EE_BS_330</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-330</field>
                    </record>
                    <record id="account_financial_report_bs_pl_cur" model="account.report.line">
                        <field name="name">Profit/Loss for the Financial Year</field>
                        <field name="code">EE_BS_331</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="account_codes_formula">-331</field>
                    </record>
                </field>
            </record>
        </field>
    </record>
</odoo>
