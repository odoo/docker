<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="l10n_ke_edi_oscu_pos.ListButtons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button t-if="displayOrders()" class="d-none d-md-block btn btn-secondary" t-on-click="this.postSelectedOrders">
                Send to eTIMS
            </button>
        </xpath>
    </t>
</templates>
