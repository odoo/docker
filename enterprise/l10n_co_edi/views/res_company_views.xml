<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_company_form_inherit_l10n_co_edi" model="ir.ui.view">
        <field name="model">res.company</field>
        <field name="name">res.company.form.inherit.l10n_co_edi</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <field name="currency_id" position="after">
                <field name="l10n_co_edi_header_actividad_economica" invisible="country_code != 'CO'"/>
            </field>
        </field>
    </record>
</odoo>
