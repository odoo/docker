			<xsd:element name="TransInd" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="PickupDate" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:date" />
