			<xsd:element name="DeclaredCurrency" type="CurrencyCode" minOccurs="0"/>
			<xsd:element name="InsuredValue" type="Money" minOccurs="0"/>
			<xsd:element name="InsuredCurrency" type="CurrencyCode" minOccurs="0"/>
